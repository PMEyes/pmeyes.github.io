{
  "rewrites": [
    {
      "source": "/data/(.*)",
      "destination": "/data/$1"
    },
    {
      "source": "/articles/(.*)",
      "destination": "/articles/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/_redirects",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
} 